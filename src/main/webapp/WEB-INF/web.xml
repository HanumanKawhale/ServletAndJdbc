<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>codingclass</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  
  
  
  <servlet>
  	<servlet-name>registration</servlet-name>
  	<servlet-class>com.codingclass.servlets.Register</servlet-class>
  	</servlet>
  
  <servlet-mapping>
  	<servlet-name>registration</servlet-name>
  	<url-pattern>/Register</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>allDetails</servlet-name>
  <servlet-class>com.codingclass.servlets.AllDetails</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>allDetails</servlet-name>
  <url-pattern>/AllDetails</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
  <servlet-name>Edit</servlet-name>
  <servlet-class>com.codingclass.servlets.Edit</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>Edit</servlet-name>
  <url-pattern>/Edit</url-pattern>
  
  </servlet-mapping>
  
  
  <servlet>
  <servlet-name>delete</servlet-name>
  <servlet-class>com.codingclass.servlets.DeleteStudent</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>delete</servlet-name>
  <url-pattern>/DeleteStudent</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>update</servlet-name>
  <servlet-class>com.codingclass.servlets.UpdateDetail</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>update</servlet-name>
  <url-pattern>/UpdateDetail</url-pattern>
  </servlet-mapping>
  
</web-app>
